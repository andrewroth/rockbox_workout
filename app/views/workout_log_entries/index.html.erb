<h1>Listing workout_log_entries</h1>

<table>
  <tr>
    <th>Workout</th>
    <th>N</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @workout_log_entries.each do |workout_log_entry| %>
  <tr>
    <td><%= workout_log_entry.workout_id %></td>
    <td><%= workout_log_entry.n %></td>
    <td><%= link_to 'Show', workout_log_entry %></td>
    <td><%= link_to 'Edit', edit_workout_log_entry_path(workout_log_entry) %></td>
    <td><%= link_to 'Destroy', workout_log_entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

